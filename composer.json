{
    "name": "sminnee/silverstripe-apikey",
    "description": "API Key management for SilverStripe",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sam Minnee",
            "email": "sam@silverstripe.com"
        }
    ],
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/graphql": "2.0.1@stable",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "silverstripe/versioned": "^1.2",
        "slevomat/coding-standard": "4.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sminnee\\ApiKey\\": "src/",
            "Sminnee\\ApiKey\\UnitTests\\": "tests/unit/"
        }
    },
    "scripts": {
        "testCoverage": "phpdbg -qrr -d memory_limit=-1 vendor/bin/phpunit --coverage-html=tests/coverage"
    }
}
